FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    axisWall
    {
        type            empty;
    }
    pipeWall
    {
        type            dynamicKistlerAlphaContactAngle;
        thetaA          90;
        thetaR          10;
        muEffKistler    muEffKistler;
        sigmaKistler    sigmaKistler;
		value           uniform 0;
    }
    vapInlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    liqInlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    topNbot
    {
        type            empty;
    }
    topNbot_pos
    {
        type            wedge;
    }
    topNbot_neg
    {
        type            wedge;
    }
}


