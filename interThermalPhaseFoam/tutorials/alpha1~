FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
     {
        type		groovyBC;
        globalScopes	( "globalVars" );
	valueExpression	"pos().x<h_in ? 1 : 0";
	value		uniform 0;
     }
    BotOutlet1
    {
        type            zeroGradient;
    }

    BotOutlet2
    {
        type            zeroGradient;
    }
   
    TopInlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    LeftVane
    {
        type            zeroGradient;
    }

    RightVane
    {
        type            zeroGradient;
    }

    FreeStream
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    } 

    FrontNBack
    {
        type            empty;
    }

    LeftSide
    {
        type            fixedValue;
        value           uniform 1;
    }
}


