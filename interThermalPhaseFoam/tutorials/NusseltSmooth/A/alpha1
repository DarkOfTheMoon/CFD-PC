FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
     {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
     }
    BotOutlet1
    {
        type            zeroGradient;
        value           uniform 0;
    }
    BotOutlet2
    {
        type            zeroGradient;
        value           uniform 0;
    }
    TopInlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    FreeStream
    {
        type            fixedValue;
        value           uniform 0;
    }
    FrontNBack
    {
        type            empty;
    }
    LeftSide
    {
        type            constantAlphaContactAngle;
        gradient        uniform 0;
        limit           gradient;
        theta0          20;
        value           uniform 1;
    }
}


