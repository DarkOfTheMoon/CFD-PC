FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Base
    {
        type            fixedValue;
        value           uniform 1;
    }

    OpenTop
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    FrontNBack
    {
        type            empty;
    }

    Sides
    {
	type            constantAlphaContactAngle;
        gradient        uniform 0;
        limit           gradient;
        theta0          45;
        value           uniform 1;
    }
}


