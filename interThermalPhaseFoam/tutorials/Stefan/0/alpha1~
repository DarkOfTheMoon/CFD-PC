FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField uniform 0; 

boundaryField
{
    BotWall
    {
        type            dynamicAlphaContactAngle;
        thetaA          90;
        thetaR          10;
        muEffKistler    muEffKistler;
        sigmaKistler    sigmaKistler;
        limit           gradient;
        value           uniform 1;
    }
    FreeStream
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    FrontNBack
    {
        type            empty;
    }
    LeftSide
    {
        type            cyclic;
    }
    RightSide
    {
        type            cyclic;
    }
}


