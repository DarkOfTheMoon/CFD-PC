FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3.2";
    object      PhaseChangeHeat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform -0;

boundaryField
{
    BotWall
    {
        type            calculated;
        value           nonuniform 0();
    }
    FreeStream
    {
        type            calculated;
        value           uniform 0;
    }
    FrontNBack
    {
        type            empty;
    }
    LeftSide
    {
        type            symmetryPlane;
    }
    RightSide
    {
        type            symmetryPlane;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform -0;
    }
}


