FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.7";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    BotWall
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    FreeStream
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    FrontNBack
    {
        type            empty;
    }
    LeftSide
    {
        type            symmetryPlane;
    }
    RightSide
    {
        type            symmetryPlane;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
}


