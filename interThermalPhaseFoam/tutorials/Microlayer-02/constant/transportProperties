FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

twoPhase
{
    transportModel  twoPhase;
    phase1          phase1;
    phase2          phase2;
}

//Water 300C
phase1
{
    transportModel  		Newtonian;
    nu              		nu [ 0 2 -1 0 0 0 0 ] 1.21E-7;
    rho             		rho [ 1 -3 0 0 0 0 0 ] 712.3;
    thermalTransportModel	Fourier;
    //lambda			lambda [1 1 -3 -1 0 0 0] 0.548;
lambda			lambda [1 1 -3 -1 0 0 0] 54.8;
    cp				cp [ 0 2 -2 -1 0 0 0] 5746;
}

phase2
{
    transportModel		Newtonian;
    nu				nu [ 0 2 -1 0 0 0 0 ] 4.256E-07;
    rho				rho [ 1 -3 0 0 0 0 0 ] 46.2;
    thermalTransportModel	Fourier;
    lambda			lambda [1 1 -3 -1 0 0 0] 0.070;
    cp				cp [ 0 2 -2 -1 0 0 0] 6220;
}

sigma           sigma [ 1 0 -2 0 0 0 0 ] 0.0144;
T_0			T_0 [0 0 0 1 0 0 0] 573.15;


thermalPhaseChange
{
	model                interfacialResistance;
	h_lv		     h_lv  [0 2 -2 0 0 0 0 ] 1405E3;
	T_sat		     T_sat [0 0 0 1 0 0 0] 573.15;
	sigmaHat	     sigmaHat [0 0 0 0 0 0 0] 1.0; //accomodation coeff. of mass transfer  [Graham 1972]   
	R_g		     R_g [0 2 -2 -1 0 0 0] 461.522; // gas constant/molar mass of liquid
	CondThresh           0.95; //Interface threshold for condensing cell pairs
	EvapThresh           0.05; //Interface threshold for evaporating cell pairs
	RelaxFac             1.0; //Under-relax phase change rate in each time step
}

surfaceTensionForce
{
	model               Brackbill;
}


