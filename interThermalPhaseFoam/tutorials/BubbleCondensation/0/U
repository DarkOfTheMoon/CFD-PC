FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    FarStream
    {
        type            zeroGradient;
    }
    Top
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    Axis
    {
        type            symmetryPlane;
    }
    FrontNBack
    {
        type            empty;
    }
    FrontNBack_pos
    {
        type            wedge;
    }
    FrontNBack_neg
    {
        type            wedge;
    }
}


