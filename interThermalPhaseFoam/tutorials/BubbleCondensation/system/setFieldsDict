FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


defaultFieldValues
(
	volScalarFieldValue alpha1 1
    volScalarFieldValue T 95
	volVectorFieldValue U (0 0 0)
);

regions
(
    sphereToCell
    {
		centre (0 4E-4 0);
        radius 2.3E-4;
        fieldValues
        (
			volScalarFieldValue alpha1 0
            volScalarFieldValue T 100
			volVectorFieldValue U (0 0 0)
        );
    }
);



// *********************************************************************** //
