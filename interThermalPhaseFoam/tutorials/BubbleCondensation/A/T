FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField uniform 96;

boundaryField
{
    Bottom
    {
        type            inletOutlet;
        inletValue      uniform 96;
        value           uniform 96;
	}

    FarStream
    {
 		type            zeroGradient;		//Adiabatic BC
    }

    Top
    {
		type            fixedValue;
		value           uniform 96;
    }

    Axis
    {
        type            symmetryPlane;
    }

    FrontNBack_pos
    {
        type            wedge;
    }

    FrontNBack_neg
    {
        type            wedge;
    }
}


