FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

twoPhase
{
    transportModel  twoPhase;
    phase1          phase1;
    phase2          phase2;
}

//Water 450K
phase1
{
    transportModel  		Newtonian;
    nu              		nu [ 0 2 -1 0 0 0 0 ] 1.71E-6;//1.71E-7;
    rho             		rho [ 1 -3 0 0 0 0 0 ] 890.47;
    thermalTransportModel	Fourier;
    lambda			lambda [1 1 -3 -1 0 0 0] 0.678;
    cp				cp [ 0 2 -2 -1 0 0 0] 4400;
}

phase2
{
    transportModel		Newtonian;
    nu				nu [ 0 2 -1 0 0 0 0 ] 3.089E-5;//3.089E-6;
    rho				rho [ 1 -3 0 0 0 0 0 ] 4.808;
    thermalTransportModel	Fourier;
    lambda			lambda [1 1 -3 -1 0 0 0] 0.0331;
    cp				cp [ 0 2 -2 -1 0 0 0] 2560;
}

sigma           sigma [ 1 0 -2 0 0 0 0 ] 0.01;//0.0429;
T_0				T_0 [0 0 0 1 0 0 0] 100;


thermalPhaseChange
{
	model                Yang;
	h_lv		         h_lv  [0 2 -2 0 0 0 0 ] 2024E3;
	T_sat		         T_sat [0 0 0 1 0 0 0]  100;
	rl					 rl	   [0 0 -1 0 0 0 0] 50000;
	rv					 rv    [0 0 -1 0 0 0 0] 50000;
}

surfaceTensionForce
{
	model               Brackbill;
}


