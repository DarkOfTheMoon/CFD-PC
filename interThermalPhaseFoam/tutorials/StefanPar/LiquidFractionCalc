FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

TotalLiquidVolume
{
    valueType internalField;
    accumulations (sum);
	expression "vol()*alpha1";
}

TotalVolume
{
    valueType internalField;
    accumulations (sum);
	expression "vol()";
}
