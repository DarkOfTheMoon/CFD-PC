FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


defaultFieldValues
(
	volScalarFieldValue alpha1 0
	volScalarFieldValue T 298
);

regions
(
	boxToCell
	{
		box (0 0 0.0005) (0.001 0.02 0);
		fieldValues
		(
			volScalarFieldValue alpha1 1
			volScalarFieldValue T 298
		);
	}
);



