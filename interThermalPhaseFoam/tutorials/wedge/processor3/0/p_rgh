FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    BotOutlet1
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              nonuniform 0();
        value           nonuniform 0();
    }
    BotOutlet2
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              nonuniform 0();
        value           nonuniform 0();
    }
    TopInlet
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    FreeStream
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    FrontNBack
    {
        type            empty;
    }
    LeftSide
    {
        type            zeroGradient;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
}


