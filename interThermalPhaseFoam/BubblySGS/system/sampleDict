FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cell;

setFormat       raw;

sets
(

    CLine
    {
        type    midPoint;
        axis    y;
        start   ( 0.001 0 		0.00002 );
        end     ( 0.001 0.002   0.00002 );
        //nPoints 200;
    }

);


fields          ( beta );


